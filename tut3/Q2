#include <iostream>
#include <string>
using namespace std;
#define MAX 100

class Stack {
    char arr[MAX];
    int top;
public:
    Stack() { top = -1; }
    void push(char c) { arr[++top] = c; }
    char pop() { return arr[top--]; }
};

int main() {
    string str = "DataStructure";
    Stack s;
    for (char c : str) s.push(c);
    for (int i = 0; i < str.length(); i++) str[i] = s.pop();
    cout << "Reversed: " << str << endl;
}
