#include <iostream>
using namespace std;
int main() {
int rows = 3, cols = 3;
int A[3][3] = {
{1, 2, 3},
{4, 5, 6},
{7, 8, 9}
};
cout << "Original Matrix:\n";
for (int i = 0; i < rows; i++) {
for (int j = 0; j < cols; j++) {
cout << A[i][j] << " ";
}
cout << endl;
}
cout << "\nTranspose Matrix:\n";
for (int i = 0; i < cols; i++) {
for (int j = 0; j < rows; j++) {
cout << A[j][i] << " ";
}
cout << endl;
}
return 0;
}
